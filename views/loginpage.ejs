    <link rel="stylesheet" type="text/css" href="styles.css">
</head>

<body class="loginko">
    <section id="header">
        <div class="logo">
            <a href="#">Logo</a>
        </div>

        <div class="navlinks">
            <div class="row">
                <a href="Ichiban_Nepal.html">Home</a>
                <a href="About.html">About</a>
                <a href="shop.html">Shop</a>
                <a href="addtocart.html"><i class="fa-solid fa-cart-shopping"></i></a>
                <a href="loginpage.html">SignUp/LogIn</a>
            </div>
        </div>
    </section>
    <div id="container1">
        <h1>Welcome</h1>
        <div id="form-container">
            <form id="login-form">
                <input type="text" id="username" placeholder="Username" required>
                <input type="password" id="password" placeholder="Password" required>
                <button type="button" id="" signup-button>Sign In</button>
            </form>
        </div>
            <div id="signup-container">
                <form id="signup-form">
                    <input type="text" id="signup-Fullname" placeholder="FullName" required>
                    <input type="text" id="signup-Email" placeholder="Email" required>
                    <input type="password" id="signup-password" placeholder="Password" required>
                    <button type="submit">Sign Up</button>
                </form>
            </div>

        <div id="signup-text" style="text-align: center;">

            Don't have an account? <button id="signup-button_hover" type="button" onclick="change()">Sign up</button>
        </div>
        <div id="signin-text" style="text-align: center;">

            Already have an account? <button id="signin-button_hover" type="button" onclick="changesignin()">Sign in</button>
        </div>
    </div>
    <script src="scripts.js"></script>

</body>

</html>